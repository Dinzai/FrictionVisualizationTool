
class Colour
{
  Colour()
  {
    r = 255;
    g = 255;
    b = 255;
  }
  
  Colour(float r, float g, float b)
  {
    this.r = r;
    this.g = g;
    this.b = b;
  }
  
  float r;
  float g;
  float b;
}

class Point
{
  Point()
  {
    x = 0;
    y = 0;
  }
  
  Point(float x, float y)
  {
    this.x = x;
    this.y = y;
  }
  
  
  float x;
  float y;
  
}
